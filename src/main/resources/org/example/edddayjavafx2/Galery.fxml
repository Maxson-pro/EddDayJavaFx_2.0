<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.edddayjavafx2.HelloController">
    <children>
        <ImageView fx:id="galleryView" fitHeight="362.0" fitWidth="602.0" pickOnBounds="true" preserveRatio="true" />
        <Button layoutX="453.0" layoutY="362.0" mnemonicParsing="false" onAction="#prevPhoto" prefHeight="38.0" prefWidth="76.0" text="Назад">
            <font><Font name="Arial Black" size="13.0" /></font>
        </Button>
        <Button layoutX="529.0" layoutY="362.0" mnemonicParsing="false" onAction="#nextPhoto" prefHeight="38.0" prefWidth="76.0" text="Вперёд">
            <font><Font name="Arial Black" size="14.0" /></font>
        </Button>
        <Button layoutX="319.0" layoutY="362.0" mnemonicParsing="false" onAction="#saveSelectedPhoto" prefHeight="38.0" prefWidth="76.0" text="Сохранить">
            <font><Font name="Arial Black" size="10.0" /></font>
        </Button>
        <Button layoutX="243.0" layoutY="362.0" mnemonicParsing="false" onAction="#selectPhotoFromFile" prefHeight="38.0" prefWidth="76.0" text="Загрузить">
            <font><Font name="Arial Black" size="10.0" /></font>
        </Button>
        <Button layoutY="362.0" mnemonicParsing="false" onAction="#backToMainList" prefHeight="38.0" prefWidth="76.0" text="к дню">
            <font><Font name="Arial Black" size="16.0" /></font>
        </Button>
        <Button layoutX="138.0" layoutY="362.0" mnemonicParsing="false" onAction="#deleteCurrentPhoto" prefHeight="38.0" prefWidth="76.0" text="Удалить">
            <font><Font name="Arial Black" size="12.0" /></font>
        </Button>
    </children>
</AnchorPane>