<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.edddayjavafx2.HelloController">
    <children>
        <Button layoutX="440.0" layoutY="354.0" mnemonicParsing="false" onAction="#openGallery" prefHeight="45.0" prefWidth="80.0" text="ФОТО" textFill="#002d80">
            <font><Font name="Arial Black" size="15.0" /></font>
        </Button>
        <Button layoutX="520.0" layoutY="354.0" mnemonicParsing="false" onAction="#exitApp" prefHeight="45.0" prefWidth="80.0" text="EX" textFill="#a41111">
            <font><Font name="Arial Black" size="18.0" /></font>
        </Button>
        <Button layoutY="354.0" mnemonicParsing="false" onAction="#goBack" prefHeight="45.0" prefWidth="73.0" text="НАЗАД">
            <font><Font name="Arial Black" size="15.0" /></font>
        </Button>
        <TabPane fx:id="tabPane" layoutY="-14.0" prefHeight="323.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" />
        <Button layoutX="200.0" layoutY="354.0" mnemonicParsing="false" onAction="#saveTab" prefHeight="45.0" prefWidth="80.0" text="Save" textFill="#2f9e2f">
            <font><Font name="Arial Black" size="18.0" /></font>
        </Button>
        <Button layoutX="280.0" layoutY="354.0" mnemonicParsing="false" onAction="#deleteTab" prefHeight="45.0" prefWidth="80.0" text="Delete" textFill="#e82b2b">
            <font><Font name="Arial Black" size="15.0" /></font>
        </Button>
        <Button layoutX="360.0" layoutY="354.0" mnemonicParsing="false" onAction="#openCalendar" prefHeight="45.0" prefWidth="80.0" text="EDD" textFill="#007c04">
            <font><Font name="Arial Black" size="18.0" /></font>
        </Button>
        <Button layoutX="127.0" layoutY="354.0" mnemonicParsing="false" onAction="#exportAndClearData" prefHeight="45.0" prefWidth="73.0" text="Donw FILE" textFill="#1430ae">
            <font><Font name="Arial Black" size="11.0" /></font>
        </Button>
      <Button layoutX="73.0" layoutY="354.0" mnemonicParsing="false" onAction="#UploadFile" prefHeight="45.0" prefWidth="54.0" text="Upload">
         <font>
            <Font name="Arial Bold Italic" size="11.0" />
         </font>
      </Button>
      <Button layoutX="520.0" layoutY="309.0" mnemonicParsing="false" onAction="#uploadDirec" prefHeight="45.0" prefWidth="80.0" text="upl">
         <font>
            <Font name="Arial Black" size="17.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
