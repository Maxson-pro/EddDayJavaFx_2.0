<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.edddayjavafx2.HelloController">
   <children>
      <Button fx:id="photo" layoutX="440.0" layoutY="354.0" mnemonicParsing="false" onAction="#Photo" prefHeight="45.0" prefWidth="80.0" text="ФОТО" textFill="#002d80">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="520.0" layoutY="354.0" mnemonicParsing="false" onAction="#Exit" prefHeight="45.0" prefWidth="80.0" text="EX" textFill="#a41111">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutY="354.0" mnemonicParsing="false" onAction="#Back" prefHeight="45.0" prefWidth="80.0" text="НАЗАД">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </Button>
       <TabPane fx:id="tabPane" prefHeight="353.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" />
      <Button fx:id="save" layoutX="200.0" layoutY="354.0" mnemonicParsing="false" onAction="#Save" prefHeight="45.0" prefWidth="80.0" text="Save" textFill="#2f9e2f">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Button>
      <Button fx:id="delete" layoutX="280.0" layoutY="354.0" mnemonicParsing="false" onAction="#Delete" prefHeight="45.0" prefWidth="80.0" text="Delete" textFill="#e82b2b">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
